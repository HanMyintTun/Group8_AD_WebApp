                                        <asp:Label ID="lblNotifContent" runat="server" CssClass="" Text='<%# (((string)(Eval("Content"))).Length>=40) ? ((string)(Eval("Content"))).Substring(0,40)+"..." : Eval("Type") %>'/>
									<span class="pull-right text-muted small"><asp:Label ID="lblNotifDateTime" runat="server" CssClass="" Text='<%# ((DateTime)Eval("NotificationDateTime")).ToString("dd-MMM-yyyy") %>'/></span>
                                    </div>
                                </asp:LinkButton></li></ItemTemplate><EmptyDataTemplate>
